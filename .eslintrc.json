{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-console": "warn",
    "max-classes-per-file": "off",
    "no-plusplus": "off",
    "class-methods-use-this": "off",
    "no-alert": "off",
    "no-new": "off",
    "no-unused-vars": "off",
    "no-restricted-globals": "off"
  }
}